{
	"name": "olebeh-music-player",
	"version": "1.0.2",
	"description": "A high quality Discord Music Player that supports YouTube, Spotify and local files (for now). Ukrainian language. Now supports CJS",
	"main": "./dist/index.js",
	"types": "./typings/index.d.ts",
	"scripts": {
		"update": "npm version patch && npm publish",
		"types": "tsc --allowJs true --declaration true --emitDeclarationOnly true --outDir ./typings",
		"compile": "tsc",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"discord",
		"music",
		"player",
		"free",
		"ukrainian",
		"high-performance"
	],
	"author": "Olebeh",
	"license": "ISC",
	"dependencies": {
		"@discordjs/opus": "^0.8.0",
		"@discordjs/voice": "^0.11.0",
		"discord.js": "^14.3.0",
		"genius-lyrics": "^4.4.0",
		"get-audio-duration": "^3.1.0",
		"play-dl": "^1.9.5",
		"tiny-typed-emitter": "^2.1.0",
		"tweetnacl": "^1.0.3"
	}
}
